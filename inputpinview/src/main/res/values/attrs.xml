<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InputPINView">
        <attr name="pinShape" format="reference" />
        <attr name="pinWidth" format="dimension" />
        <attr name="pinHeight" format="dimension" />
        <attr name="pinLength" format="integer" />
        <attr name="pinTextStyle" format="reference" />
        <attr name="pinTextColor" format="color" />
        <attr name="pinMargin" format="dimension" />
        <attr name="pinEyePassword" format="reference" />
        <attr name="pinPasswordAnimate" format="integer" />
        <attr name="pinPasswordSymbol" format="string" />
        <attr name="pinType" format="enum">
            <enum name="number" value="0" />
            <enum name="password" value="1" />
            <enum name="passwordWithEye" value="2" />
        </attr>
    </declare-styleable>


</resources>